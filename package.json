{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "bunyan": "^1.8.14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "bunyan-gelf-formatter",
  "author": "Ihor Diachenko",
  "module": "dist/bunyan-gelf-formatter.esm.js",
  "size-limit": [
    {
      "path": "dist/bunyan-gelf-formatter.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/bunyan-gelf-formatter.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/flat": "^5.0.1",
    "bunyan": "^1.8.14",
    "husky": "^4.3.0",
    "mockdate": "^3.0.2",
    "stream-mock": "^2.0.5",
    "tsdx": "^0.14.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "error-stack-parser": "^2.0.6",
    "flat": "^5.0.2"
  }
